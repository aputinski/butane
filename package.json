  },
  "devDependencies": {
    "babel": "5.2.12",
    "chai": "2.3.0",
    "mocha": "2.2.4",
    "standard": "~3.7.3"
  },
  "license": "MIT",
  "standard": {
    "ignore": [
      "dist/**"
    ]
